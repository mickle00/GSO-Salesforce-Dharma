    <fields>
        <fullName>Total_Load_Time__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( 
ROUND( 
(Contract_Complete_Date__c - Contract_Start_Loading_Date__c) - 
MOD(Contract_Complete_Date__c - Contract_Start_Loading_Date__c,1) 
,0) 
) 
&amp;&quot; day(s), &quot;&amp; 
TEXT( 
ROUND( 
MOD(Contract_Complete_Date__c - Contract_Start_Loading_Date__c,1)*24 - 
MOD(MOD(Contract_Complete_Date__c - Contract_Start_Loading_Date__c,1)*24,1) 
,0) 
) 
&amp;&quot; hour(s), &quot;&amp; 
TEXT( 
ROUND( 
MOD(MOD(Contract_Complete_Date__c - Contract_Start_Loading_Date__c,1)*24,1) 
*60 
,0) 
)&amp;&quot; minute(s)&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Load Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_MM_Time__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( 
ROUND( 
(MM_MC_Returned_to_Loading_Team__c - Returned_to_MM_MC_Date__c) - 
MOD(MM_MC_Returned_to_Loading_Team__c - Returned_to_MM_MC_Date__c,1) 
,0) 
) 
&amp;&quot; day(s), &quot;&amp; 
TEXT( 
ROUND( 
MOD(MM_MC_Returned_to_Loading_Team__c - Returned_to_MM_MC_Date__c,1)*24 - 
MOD(MOD(MM_MC_Returned_to_Loading_Team__c - Returned_to_MM_MC_Date__c,1)*24,1) 
,0) 
) 
&amp;&quot; hour(s), &quot;&amp; 
TEXT( 
ROUND( 
MOD(MOD(MM_MC_Returned_to_Loading_Team__c - Returned_to_MM_MC_Date__c,1)*24,1) 
*60 
,0) 
)&amp;&quot; minute(s)&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total MM Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
