    <validationRules>
        <fullName>Contract_Loading_Complete</fullName>
        <active>true</active>
        <description>Once a contract status has been set to Contact Loading Complete, it cannot be changed.</description>
        <errorConditionFormula>AND(ISCHANGED( Status__c ), ISPICKVAL(PRIORVALUE( Status__c ), &quot;Contract Loading Complete&quot;), NOT(ISPICKVAL (Status__c, &quot;Contract Loading Complete&quot;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status for completed contracts cannot be changed. Please send your request to hotelinfo@hotels.com for any changes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cut_off_policy</fullName>
        <active>true</active>
        <description>The cut off policy reason field must be populated if the cut off policy is not 0</description>
        <errorConditionFormula>AND( 
NOT( 
OR( 
ISPICKVAL(Cut_Off_Policy__c, &quot;0&quot;), 
ISBLANK(TEXT(Cut_Off_Policy__c)) 
)), 
ISBLANK( Cut_Off_Policy_Reason__c ) 
)</errorConditionFormula>
        <errorDisplayField>Cut_Off_Policy_Reason__c</errorDisplayField>
        <errorMessage>Please provide a reason if Cut-Off Policy is not 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Property_Submitted_By_for_LATAM_Region</fullName>
        <active>false</active>
        <description>Require the Property Submitted By Contact field for properties in the LATAM Markets Region.</description>
        <errorConditionFormula>AND( 
ISNEW(), 
OR( Account__r.Region__r.Name = &quot;Mexico &amp; Central America&quot;, 
 Account__r.Region__r.Name =&quot;South America&quot;), 
ISBLANK( Property_Submitted_By_Contact__c ), 
OR( 
ISPICKVAL(Checklist_Type__c , &quot;Independent - New&quot;), 
ISPICKVAL(Checklist_Type__c , &quot;Specialty Lodging - New&quot;) 
) 
)</errorConditionFormula>
        <errorDisplayField>Property_Submitted_By_Contact__c</errorDisplayField>
        <errorMessage>New contracts for LATAM Properties require a Property Submitted By Contact</errorMessage>
    </validationRules>
